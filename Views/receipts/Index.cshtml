@model IEnumerable<WebApplication1.Models.receipt>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.dateofCreation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.totalPrice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Staff.nameStaff)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.dateofCreation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.totalPrice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Staff.nameStaff)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idReceipt }) |
            @Html.ActionLink("Details", "Details", new { id=item.idReceipt }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idReceipt })
        </td>
    </tr>
}

</table>
